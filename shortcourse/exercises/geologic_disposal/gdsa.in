#=========================== saturation functions =============================
#this never gets used because fully saturated
SATURATION_FUNCTION default
  SATURATION_FUNCTION_TYPE VAN_GENUCHTEN
  RESIDUAL_SATURATION 0.1d0
  LAMBDA 0.5
  ALPHA 1.d-4
END

#=========================== flow conditions ==================================
#hydrostatic pressure gradient and geothermal gradient for initial conditions
FLOW_CONDITION initial #also BC for top, bottom, west, east; north and south no flow
  TYPE
    PRESSURE DIRICHLET
    TEMPERATURE DIRICHLET
  /
  PRESSURE DATASET 1d_pressure
  TEMPERATURE DATASET 1d_temperature
END

#heat source for each waste type

FLOW_CONDITION hlw
  TYPE
    RATE MASS_RATE
    ENERGY_RATE SCALED_ENERGY_RATE VOLUME #distribute heat source over all cells in region
  /
  !SYNC_TIMESTEP_WITH_UPDATE
  INTERPOLATION LINEAR
  RATE 0.d0 
  ENERGY_RATE FILE ./hlw_heat.txt
END

FLOW_CONDITION oxide
  TYPE
    RATE MASS_RATE
    ENERGY_RATE SCALED_ENERGY_RATE VOLUME #distribute heat source over all cells in region
  /
  !SYNC_TIMESTEP_WITH_UPDATE
  INTERPOLATION LINEAR
  RATE 0.d0 
  ENERGY_RATE FILE ./oxide_heat.txt
END

FLOW_CONDITION metallic
  TYPE 
    RATE MASS_RATE
    ENERGY_RATE SCALED_ENERGY_RATE VOLUME #distribute heat source over all cells in region
  /
  INTERPOLATION LINEAR
  RATE 0.d0 
  ENERGY_RATE FILE ./metallic_heat.txt
END

#=========================== transport conditions and constraints =============================
TRANSPORT_CONDITION bottom 
  TYPE zero_gradient
  CONSTRAINT_LIST
    0.d0 initial
  /
END
TRANSPORT_CONDITION initial
  TYPE dirichlet_zero_gradient
  CONSTRAINT_LIST
    0.d0 initial
  /
END
CONSTRAINT initial
  CONCENTRATIONS    
skip
    Am241 1.d-21 T
    Am243 1.d-21 T
    Pu238 1.d-21 T
    Pu239 1.d-21 T
    Pu240 1.d-21 T
    Pu242 1.d-21 T
    Np237 1.d-21 T
    U233  1.d-21 T
    U234  1.d-21 T
    U236  1.d-21 T
