    Th229 1.d-21 T
    Th230 1.d-21 T
    Tc99  1.d-21 T 
noskip
    I129  1.d-21 T
    Cs135 1.d-21 T
  /
  MINERALS
skip
    Am241(s) 0.d0  1.d0
    Am243(s) 0.d0  1.d0
    Pu238(s) 0.d0  1.d0
    Pu239(s) 0.d0  1.d0
    Pu240(s) 0.d0  1.d0
    Pu242(s) 0.d0  1.d0
    Np237(s) 0.d0  1.d0
    U233(s) 0.d0  1.d0
    U234(s) 0.d0  1.d0
    U236(s) 0.d0  1.d0
    U238(s) 0.d0  1.d0
    Th229(s) 0.d0  1.d0
    Th230(s) 0.d0  1.d0
    Tc99(s) 0.d0  1.d0
noskip
    I129(s)  0.d0  1.d0
    Cs135(s) 0.d0  1.d0
  /
END
#=========================== regions ==========================================
#REGION all, faces, and everything else are in:
EXTERNAL_FILE ./regions.txt

#Observation points are in:
EXTERNAL_FILE ./obs_regions.txt

#======================= boundary condition couplers ===================================
BOUNDARY_CONDITION top
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION top
END

BOUNDARY_CONDITION bottom
  FLOW_CONDITION initial 
  TRANSPORT_CONDITION bottom 
  REGION bottom
END

BOUNDARY_CONDITION west
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION west
END

BOUNDARY_CONDITION east
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION east
END

#============================= initial condition couplers ==================================
INITIAL_CONDITION all
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION all
END

#============================= source_sink couplers ========================================
#heat source couplers in here:
EXTERNAL_FILE ./source_sink.txt

#============================= strata ======================================================
#strata in here:
EXTERNAL_FILE ./strata.txt

END_SUBSURFACE

