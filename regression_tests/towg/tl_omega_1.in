#  OpenGoSim 4-Oct-2017 
#  Todd-Longstaff - 1D case - omega=1
SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW Flow
      MODE TODD_LONGSTAFF
      OPTIONS
       ISOTHERMAL
       TL_OMEGA 1.0
      /
    / ! end of subsurface_flow
  / ! end of process models
  !CHECKPOINT
  !/ 
END  !! end simulation block

SUBSURFACE
#=========================== regression =======================================
REGRESSION
  CELLS
    1
    2
    3
    10
  /
END
#=========================== discretization ===================================

! PV~10E5 m3
GRID
  TYPE structured
  ORIGIN 0.d0 0.d0 -1000.d0
  NXYZ 10 1 1
  DXYZ
    100.0
    100.0
    10.0
  END
END

#=========================== solver options ===================================
NEWTON_SOLVER FLOW
  ATOL 1D-8
  RTOL 1D-8
  STOL 1D-30
  DTOL 1D15
  ITOL 1D-7
  MAXIT 8
END

#=========================== times ============================================
TIME
  FINAL_TIME 2000 d
  INITIAL_TIMESTEP_SIZE 1.0d-5 d
  MINIMUM_TIMESTEP_SIZE 1.0D-10 d
  MAXIMUM_TIMESTEP_SIZE 10.0 d
  !MAXIMUM_TIMESTEP_SIZE 1 d at 0. d
END

#=========================== output options ===================================
skip
OUTPUT
  FORMAT HDF5
  TIMES d 0.0 0.1 0.9 1.0 5.0 20.0 27.0 100.0 200 500 1000
  MASS_BALANCE_FILE
    PERIODIC TIMESTEP 1  
  END
  OBSERVATION_FILE
    PERIODIC TIMESTEP 1
    VARIABLES
      LIQUID_SATURATION
      LIQUID_DENSITY
      LIQUID_MOBILITY
      LIQUID_VISCOSITY
      OIL_PRESSURE
      OIL_SATURATION
      OIL_DENSITY
      OIL_MOBILITY
      OIL_VISCOSITY
      TEMPERATURE
      POROSITY
      PERMEABILITY_X
    /
  END 
END
noskip
#=========================== material properties ==============================

MATERIAL_PROPERTY formation
  ID 1
  POROSITY 0.1d0
  TORTUOSITY 1.d0
  ROCK_DENSITY 2.350d3
  SPECIFIC_HEAT 1.0d3
  THERMAL_CONDUCTIVITY_DRY 1.541d0
  THERMAL_CONDUCTIVITY_WET 1.541d0 
  SOIL_COMPRESSIBILITY_FUNCTION QUADRATIC
  SOIL_COMPRESSIBILITY 1.0d-10 ! 1/Pa
  SOIL_REFERENCE_PRESSURE 1.0D5
  CHARACTERISTIC_CURVES ch1
  PERMEABILITY
    PERM_X 0.9869D-12 !1000 mD 
    PERM_Y 0.9869D-12 !1000 mD
    PERM_Z 0.9869D-12 !1000 mD
  /
/

#=========================== datasets ==============================
#=========================== saturation functions =============================
CHARACTERISTIC_CURVES ch1
! This is just for capillary pressure, and is set to zero here
  SATURATION_FUNCTION BROOKS_COREY
    LAMBDA 0.4d0
    ALPHA 0.5e-03
    LIQUID_RESIDUAL_SATURATION 0.0d0
    MAX_CAPILLARY_PRESSURE 0.d0 ! neglect capillary pressure effects 
  /
! This is Krw, here with Swcr=0
  PERMEABILITY_FUNCTION BURDINE_VG_LIQ
   M 3.0
   LIQUID_RESIDUAL_SATURATION 0.0
  END
  PERMEABILITY_FUNCTION MOD_BC_OIL
    M 2.0d0
    LIQUID_RESIDUAL_SATURATION 0.d0 
    OIL_RESIDUAL_SATURATION 0.0d0
    GAS_RESIDUAL_SATURATION 0.0d0
  /
  PERMEABILITY_FUNCTION BURDINE_BC_GAS
    PHASE GAS
    LIQUID_RESIDUAL_SATURATION 0.0 
    GAS_RESIDUAL_SATURATION 0.0d0
    LAMBDA 0.7
  /
  !TEST
/

#=========================== EOSs =============================================

EOS WATER
  DENSITY CONSTANT 1000.00d0
  VISCOSITY CONSTANT 1.0d-3 ! 1 cp
END

EOS OIL
  FORMULA_WEIGHT 1000.0d0
  DENSITY INVERSE_LINEAR ! den = den0 / [1- C(p-p1)] [1 + E(t-t1)]
    REFERENCE_VALUE 1000.00d0           !den0 [kg/m3]  
    PRES_REF_VALUE 100000               !p1 [Pa]
    TEMP_REF_VALUE 15.0d0               !t1 [°C]
    COMPRESS_COEFF 1.0d-10              !C [1/Pa]=0.0001 1/Bar
    THERMAL_EXPANSION_COEFF 0.0         !E [1/°C]
  END
  ENTHALPY QUADRATIC_TEMP ! h = c1*(t-t1) + 0.5*c2*(t-t2)
    TEMP_REF_VALUES   15.6d0  15.6d0    !t1, t2 [°C]
    TEMP_COEFFICIENTS  0.0     0.0d0    !c1, c2 [J/kg/°C]
  END
  VISCOSITY CONSTANT 1.0d-3 ! 1 cp
END

EOS GAS
  FORMULA_WEIGHT 16.04d0
  DENSITY   DEFAULT
  VISCOSITY CONSTANT 2.0d-5 ! 0.02 cp
END
#=========================== regions ==========================================
REGION all
COORDINATES
-1.d20 -1.d20 -1.d20
 1.d20  1.d20  1.d20 
/
/

REGION inj1
 BLOCK  1 1 1 1 1 1 
END

REGION EAST
  FACE EAST
  COORDINATES
    1000   0.d0 -900.d0
    1000 100.d0 -1900.d0
  /
END

REGION obs_inje
 BLOCK  1 1 1 1 1 1 
END


REGION obs_prod
 BLOCK  10 10 1 1 1 1 
END


#=========================== observation ============================
skip
OBSERVATION
  REGION obs_inje
END

OBSERVATION
  REGION obs_prod
END
noskip
#=========================== wells ==================================

! Rate is 100 rm3/day - so 1 PV in 1000 days

FLOW_CONDITION gas_injector_cond
  TYPE
    RATE volumetric_rate
    TEMPERATURE dirichlet
  /
  RATE 0.0000000 0.000000 100 0 m^3/day m^3/day m^3/day M
  TEMPERATURE 15.0 C
/

#=========================== flow conditions ==================================
FLOW_CONDITION initial_press
  TYPE
    OIL_PRESSURE dirichlet
    OIL_SATURATION dirichlet
    GAS_SATURATION dirichlet
    TEMPERATURE dirichlet
  /
  OIL_PRESSURE 20.0D6 !200bars 
  OIL_SATURATION 1.0d0 !With this accounts for connate water 
  GAS_SATURATION 0.0d0 !
  TEMPERATURE 15.0d0 C  !up to here it refers to primary vars
  DATUM 0.d0 0.d0 -1530.0d0
  !define OWC and WATER_PRESSURE_GRAD as new optional sub_condition 
  !OWC 0.0d0 0.d0 -3000.0d0 ! oil water contact below domain, e.g. oil region
/

#=========================== condition couplers ===============================
INITIAL_CONDITION initial
FLOW_CONDITION initial_press
REGION all
/

SOURCE_SINK gas_injection1
  FLOW_CONDITION gas_injector_cond
  REGION inj1
END

BOUNDARY_CONDITION producer
  FLOW_CONDITION initial_press
  REGION EAST
END

#=========================== stratigraphy couplers ============================
STRATA
  REGION all
  MATERIAL formation
END

END_SUBSURFACE

